#-------------------------------------------------------
# graphics in tile format
#-------------------------------------------------------
-gt

#-------------------------------------------------------
# exclude map from output
#-------------------------------------------------------
-m!

#-------------------------------------------------------
# graphics bit depth is 8 (256 color)
#-------------------------------------------------------
-gB8

#-------------------------------------------------------
# metatile dimensions
#-------------------------------------------------------
-Mh 2
-Mw 2

#-------------------------------------------------------
# include palette in output
#-------------------------------------------------------
-p

#-------------------------------------------------------
# use word arrays
#-------------------------------------------------------
-U32